{
  "name": "vue-enhanced-swiper",
  "version": "1.0.0",
  "description": "Enhanced mobile swiper component for Vue 3 with scroll conflict resolution",
  "keywords": [
    "vue",
    "vue3",
    "swiper",
    "mobile",
    "touch",
    "carousel",
    "slider",
    "scroll-conflict"
  ],
  "main": "./dist/cjs/vue-enhanced-swiper.cjs",
  "module": "./dist/esm/vue-enhanced-swiper.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/vue-enhanced-swiper.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/vue-enhanced-swiper.cjs"
      }
    },
    "./style.css": "./dist/style.css",
    "./core.css": "./dist/core.css"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite --config vite.dev.config.ts",
    "build": "vite build",
    "build:lib": "vite build",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly",
    "build:dev": "vite build --mode development",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "prepublishOnly": "npm run build && npm run type-check",
    "release": "node scripts/release.js patch",
    "release:minor": "node scripts/release.js minor",
    "release:major": "node scripts/release.js major",
    "release:beta": "node scripts/release.js patch beta",
    "release:dry": "node scripts/release.js patch latest --dry-run",
    "publish:patch": "bash scripts/publish.sh patch",
    "publish:minor": "bash scripts/publish.sh minor",
    "publish:major": "bash scripts/publish.sh major",
    "publish:dry": "bash scripts/publish.sh patch latest --dry-run"
  },
  "peerDependencies": {
    "swiper": "^10.0.0",
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.12.0",
    "@types/node": "^20.8.0",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vitest": "^0.5.4",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.3",
    "sass": "^1.69.0",
    "swiper": "^10.0.0",
    "terser": "^5.43.1",
    "typescript": "~5.2.0",
    "vite": "^4.4.11",
    "vite-plugin-dts": "^3.6.0",
    "vitepress": "^1.0.0-rc.20",
    "vitest": "^0.34.6",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.19"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zhangnuli/vue-enhanced-swiper.git"
  },
  "bugs": {
    "url": "https://github.com/zhangnuli/vue-enhanced-swiper/issues"
  },
  "homepage": "https://github.com/zhangnuli/vue-enhanced-swiper#readme",
  "author": {
    "name": "Vue Enhanced Swiper Team",
    "email": "contact@vue-enhanced-swiper.com",
    "url": "https://vue-enhanced-swiper.github.io"
  },
  "license": "MIT"
}
